package com.example.demo.service;

import com.example.demo.model.ObjectAttributeArray;
import com.example.demo.model.ObjectAttributeSingleSet;

public class FormalConceptAnalysis {

	public void fca(ObjectAttributeArray vals) {
		
		ObjectAttributeArray objectAttributeArray = new ObjectAttributeArray();
		
		int arraySize = vals.getObjectAttributeSingleSet().size();
		
		objectAttributeArray.getObjectAttributeSingleSet().add(vals.getObjectAttributeSingleSet().get(0));
		
		for (int i=0; i< arraySize; i++) {
			for (int j=i+1; j< arraySize; j++) {
				if (objectAttributeArray.getObjectAttributeSingleSet().get(i).getAttribute().size() <= objSet.getAttribute().size()) {
					objectAttributeArray.getObjectAttributeSingleSet().remove(i);
					objectAttributeArray.getObjectAttributeSingleSet().add(i, objSet);
				}
			}
		}
	}
}
